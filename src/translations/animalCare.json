    "sections": {
      "cowCare": {
        "title": "Cow Care Guide",
        "subsections": {
          "feeding": {
            "title": "Feeding Requirements",
            "content": "Cows in dry climate need 15-20 kg of fodder daily. During dry season, provide stored hay, silage, and supplements. Ensure access to clean water at all times. Include mineral supplements (salt, phosphorus, calcium) in diet."
          },
          "shelter": {
            "title": "Shelter Design",
            "content": "Build open-sided sheds with proper ventilation. Height should be 10-12 feet. Use white/light colored roof to reflect heat. Provide shade area for hot months. Ensure drainage to prevent water stagnation."
          },
          "health": {
            "title": "Health Management",
            "content": "Vaccinate against FMD, HS, and BQ annually. Regular deworming every 3 months. Watch for signs of mastitis, foot rot, and bloat. Maintain clean surroundings and proper hygiene."
          },
          "breeding": {
            "title": "Breeding Practices",
            "content": "First breeding at 18-24 months. Calving interval should be 12-14 months. Use AI or natural breeding. Keep detailed records of breeding dates and outcomes."
          },
          "production": {
            "title": "Milk Production Tips",
            "content": "Provide quality feed and clean water. Regular milking (twice daily) improves production. Maintain stress-free environment. Average yield: 5-8 liters per day depending on breed."
          }
        }
      },
      "poultryCare": {
        "title": "Poultry Care (Hen, Duck, Chicken)",
        "subsections": {
          "feeding": {
            "title": "Feed Formulation",
            "content": "Layers need 16-18% protein diet. Provide balanced feed with grains, pulses, and supplements. Include calcium for egg production. Fresh water available 24/7. Feed cost: Rs. 8-12 per bird daily."
          },
          "housing": {
            "title": "Housing in Extreme Heat",
            "content": "Build well-ventilated sheds with 4-5 sq ft per bird. Use bamboo/wooden slats for flooring. Provide shade cloth and sprinklers. Ensure 12-14 hours of light for egg production."
          },
          "disease": {
            "title": "Disease Management",
            "content": "Vaccinate against Ranikhet, Fowl Pox. Watch for signs of coccidiosis, worms. Maintain biosecurity. Quarantine sick birds. Clean housing weekly."
          },
          "eggProduction": {
            "title": "Egg Production",
            "content": "Good layers produce 250-300 eggs/year. Peak production at 6-18 months age. Provide nesting boxes (1 per 3-4 birds). Collect eggs twice daily."
          },
          "waterManagement": {
            "title": "Water Management",
            "content": "Provide clean water in hot climate. Use nipple drinkers to prevent contamination. Change water 2-3 times daily. Ensure adequate water supply during heat stress."
          }
        }
      },
      "fishFarming": {
        "title": "Fish Farming (Aquaculture)",
        "subsections": {
          "pondManagement": {
            "title": "Pond Management",
            "content": "Prepare pond by removing predatory fish. Lime application: 250 kg/acre. Stock density: 5000-8000 fingerlings/acre. Maintain water depth: 1-1.5 meters."
          },
          "feedTypes": {
            "title": "Feed Types",
            "content": "Use pellet feed with 30-35% protein. Feed twice daily at 2-3% body weight. Include vegetable waste, rice bran. Cost-effective feeding improves profit margin."
          },
          "diseaseControl": {
            "title": "Disease Control",
            "content": "Monitor water quality (pH 6.5-8.5). Watch for fish diseases like gill rot, fin rot. Use lime and salt for treatment. Maintain proper aeration."
          },
          "waterQuality": {
            "title": "Water Quality",
            "content": "Dissolved oxygen: 5-8 ppm. pH: 6.5-8.5. Temperature: 25-32¬∞C. Ammonia: <0.1 ppm. Test water monthly."
          },
          "harvesting": {
            "title": "Harvesting",
            "content": "Harvest at 6-8 months (500-800g). Use nets for selective harvesting. Drain pond partially for easy catching. Store in cool place with ice."
          }
        }
      },
      "snakeManagement": {
        "title": "Snake Management",
        "subsections": {
          "safety": {
            "title": "Safety Precautions",
            "content": "Wear boots and long pants. Use stick to move grass. Never corner a snake. Stay calm and move away slowly. Seek medical help for bites immediately."
          },
          "identification": {
            "title": "Identification",
            "content": "Common snakes: Cobra (hood), Viper (triangular head), Rat snake (non-venomous). Learn to identify before action. Most snakes are harmless."
          },
          "prevention": {
            "title": "Prevention Methods",
            "content": "Keep surroundings clean. Remove rodents (snake food). Seal holes and gaps. Use snake repellent plants (marigold, garlic). Maintain clear pathways."
          },
          "authorities": {
            "title": "When to Contact Authorities",
            "content": "Contact local forest office or wildlife rescue. Report venomous snakes near habitation. Get professional help for removal. Never attempt to kill snakes."
          }
        }
      }
    },
